{
  "name": "sanskriti-hamburg-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspace=packages/types && npm run build --workspace=apps/webapp",
    "start": "npm run start --workspace=apps/webapp",
    "lint": "npm run lint --workspace=apps/webapp",
    "netlify:dev": "npm run netlify:dev --workspace=apps/webapp"
  },
  "devDependencies": {
    "typescript": "~4.9.5"
  }
}